{
  "name": "alt-console",
  "version": "1.7.0",
  "description": "",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenBrostoff/alt-log.git"
  },
  "scripts": {
    "test": "mocha lib/*.spec.js",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ."
  },
  "author": "Ben Brostoff",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "prettier": "^1.4.4",
    "sinon": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-console": [
        "off"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "lib/index.spec.js"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "chalk": "^2.1.0"
  }
}
